{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "description": "",
    "license": "BSL-1.0",
    "maintainers": [
        "tonthat.quocminh@gmail.com"
    ],
    "name": "pbatoolkit",
    "version": "0.0.10",
    "dependencies": [
        {
            "name": "range-v3",
            "version>=": "0.12.0"
        },
        {
            "name": "fmt",
            "version>=": "11.0.2"
        },
        {
            "name": "tbb",
            "version>=": "2022.0.0"
        }
    ],
    "features": {
        "suitesparse": {
            "description": "Use SuiteSparse for sparse decompositions",
            "dependencies": [
                {
                    "name": "suitesparse-cholmod",
                    "version>=": "5.3.0",
                    "features": [
                        "matrixops",
                        "modify",
                        "openmp",
                        "partition",
                        "supernodal"
                    ]
                }
            ]
        },
        "mkl": {
            "description": "Use Intel MKL for dense and sparse linear algebra",
            "dependencies": [
                {
                    "name": "intel-mkl",
                    "version>=": "2023.0.0"
                }
            ]
        },
        "metis": {
            "description": "Use Metis for graph partitioning",
            "dependencies": [
                {
                    "name": "metis",
                    "version>=": "2022-07-27"
                }
            ]
        },
        "cuda": {
            "description": "Use CUDA for GPU accelerated modules",
            "dependencies": [
                {
                    "name": "cuda-api-wrappers",
                    "version>=": "0.8.0"
                }
            ]
        }
    },
    "builtin-baseline": "b02e341c927f16d991edbd915d8ea43eac52096c"
}