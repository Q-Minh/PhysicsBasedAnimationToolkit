        "tonthat.quocminh@gmail.com"
    ],
    "name": "pbatoolkit",
    "version": "0.0.10",
    "dependencies": [
        {
            "name": "range-v3",
            "version>=": "0.12.0"
        },
        {
            "name": "fmt",
            "version>=": "10.2.1"
        },
        {
            "name": "tbb",
            "version>=": "2022.0.0"
        },
        {
            "name": "doctest",
            "version>=": "2.4.11"
        }
    ],
    "features": {
        "suitesparse": {
            "description": "Use SuiteSparse for sparse decompositions",
            "dependencies": [
                {
                    "name": "suitesparse",
                    "version>=": "5.8.0#2"
                }
            ]
        },
        "mkl": {
            "description": "Use Intel MKL for dense and sparse linear algebra",
            "dependencies": [
                {
                    "name": "intel-mkl",
                    "version>=": "2023.0.0"
                }
            ]
        },
        "metis": {
            "description": "Use Metis for graph partitioning",
            "dependencies": [
                {
                    "name": "metis",
                    "version>=": "2022-07-27"
                }
            ]
        },
        "cuda": {
            "description": "Use CUDA for GPU accelerated modules",
            "dependencies": [
                {
                    "name": "cuda-api-wrappers",
                    "version>=": "0.7.1"
                }
            ]
        }
    },
    "builtin-baseline": "060c2708a31314fad5af86ca5a1fc5009e3bddce"
