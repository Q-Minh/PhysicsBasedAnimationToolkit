{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "description": "",
    "license": "BSL-1.0",
    "maintainers": [
        "tonthat.quocminh@gmail.com"
    ],
    "name": "pbatoolkit",
    "version": "0.0.10",
    "dependencies": [
        {
            "name": "range-v3",
            "version>=": "0.12.0"
        },
        {
            "name": "fmt",
            "version>=": "10.2.1"
        },
        {
            "name": "tbb",
            "version>=": "2021.11.0"
        },
        {
            "name": "doctest",
            "version>=": "2.4.11"
        }
    ],
    "features": {
        "suitesparse": {
            "description": "Use SuiteSparse for sparse decompositions",
            "dependencies": [
                {
                    "name": "suitesparse",
                    "version>=": "5.8.0#2"
                }
            ]
        },
        "mkl": {
            "description": "Use Intel MKL for dense and sparse linear algebra",
            "dependencies": [
                {
                    "name": "intel-mkl",
                    "version>=": "2023.0.0"
                }
            ]
        },
        "python": {
            "description": "Enable building Python bindings",
            "dependencies": [
                {
                    "name": "pybind11",
                    "version>=": "2.11.1"
                }
            ]
        },
        "cuda": {
            "description": "Use CUDA for GPU accelerated modules",
            "dependencies": [
                {
                    "name": "cuda-api-wrappers",
                    "version>=": "0.6.7"
                }
            ]
        }
    },
    "builtin-baseline": "055721089e8037d4d617250814d11f881e557549"
}